 function showQualifiersDescription(e){
  var qualifiersData=jQuery('.qualifiersData',e.parentNode).attr('value').trim();
  var qualifiersMap=new Array();
  qualifiersData=qualifiersData.substr(1,qualifiersData.length-1);
  var splitQualifier=qualifiersData.split('QualifiersData:');
  for(var k=1;k<splitQualifier.length;k++){
   var qualifier=new Object();
   var qualifierStr=splitQualifier[k];
   qualifierStr=qualifierStr.replace(/^\s+|\s+$/g,'').substr(1,(qualifierStr.length-3))
   var coord1=(qualifierStr.indexOf('description='));
   var coord2=(qualifierStr.indexOf(', imgUrL=') );
         var description=qualifierStr.substr(coord1+12,coord2-12);
         var imgURL='/servlet/servlet.FileDownload?file='+qualifierStr.substr(coord2+9,(qualifierStr.length-2)).split(', name=')[0];
