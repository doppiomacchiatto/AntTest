 //this function scroll the tierts into post to manage the slide in the blue navigation bar at top of the pod 
 /*
 * param isNext: if true the movement must be right to left else left to right
 * param parent: is used to determinate wich pod are refrence to slide. if you add a feature that use this funcion the parent must be contestPodWrapper DOM element. see foundParents funtion at bottom
 * param newIndex: is used to tell if u scroll more that one position 
 * param imperativeScroll: is use to allow scroll to the last position when start the page
 */
 function moveSlide(isNext,parent,newIndex,imperativeScroll){
  var slideSize=jQuery(".TiersScrolleable .slide",jQuery(parent)).size();
 //jQuery('.contestPodScrollSynchronize',jQuery(parent)).val() this value store if any animation is still executing, if we doesn't use that, jQuery.animate allow multi click and the scroll doesn't work 
  if(jQuery('.contestPodScrollSynchronize',jQuery(parent)).val()!='true')return;
  if(slideSize>1){
   var diff=1;
   //jQuery('.contestPodScrollIndex',jQuery(parent) store the currentIndex of the scroll
   var index=parseInt(jQuery('.contestPodScrollIndex',jQuery(parent)).val());
   //ensure that button with index==1 is unable to be clicked if that is not full displayed
   // Example if you are in lvl 4, see a little of level 1 button if do click in this part this function ignore the event
   if (!imperativeScroll){
    if (index==slideSize || index==1){
     if (Math.abs(index - newIndex) >2) return;
    }else{
     if (Math.abs(index - newIndex) >1) return;
    }
   }
   
   if((newIndex!=null && newIndex!=0 && newIndex!=undefined )){
    if(index==newIndex && !imperativeScroll )return;
    diff=(newIndex-index==0)?1:newIndex-index;
    diff=(diff<0)?(diff*-1):diff;
   }else{
    newIndex=(isNext)?(index+1):(index-1);
   }
    
   var newMargin=parseInt(jQuery('.TiersScrolleable .items',jQuery(parent)).css('left').replace('px',''));
   var width=parseInt(jQuery('.TiersScrolleable .slide',jQuery(parent)).css('width').replace('px',''))+4;
   var maxScroll=(slideSize*width)-width;
   newMargin=(isNext)?newMargin-(width*diff):newMargin+(width*diff);
   //controll the bounds of the scroll
   if(((newMargin)>0 || newMargin<(-1*maxScroll)) && ((newIndex<1 || newIndex>slideSize) && !imperativeScroll)) return;
   //update current index
   jQuery('.contestPodScrollIndex',jQuery(parent)).val(newIndex);
   //this function manage the position of the blue navigation bar
   slideBluelevels(jQuery(parent),newIndex,index,imperativeScroll);
   //
   var slideBelow3=false;
    if(slideSize<3){
     slideBelow3=true;
     jQuery(jQuery('.cba',parent)[1]).css( {"display":'none'});
    }
   //manage arrows and the (dark/light) color of the dots 
    if(newIndex==1){
     jQuery('.contestPodSuspensionPointsRight',parent).css('background-color',getColorBackground(newIndex,parent));
    
     jQuery('.contestPodSuspensionPointsLeft',parent).css('background-color',getColorBackground(newIndex+((slideBelow3)?1:2),parent));
     jQuery('.contestPodLevelWrapper',parent).css('background-color',getColorBackground(newIndex+((slideBelow3)?1:2),parent));
    //visibility must be '' when you want that the arrow be visible, because if that attribute is 'visible' and drag and drop pod generate a issue 
     jQuery('.contestPodNextTier',parent).css( {"visibility":''});
       jQuery('.contestPodPrevTier',parent).css( {"visibility":'hidden'});
    jQuery(jQuery('.cba',parent)[0]).addClass('cactive'); 
    jQuery(jQuery('.cba',parent)[1]).removeClass('cactive');  
    jQuery(jQuery('.cba',parent)[2]).removeClass('cactive');  
   }else if(newIndex>=2 && newIndex<slideSize){
    var indexToFollowColorBg=1;
    if(newIndex>2){
     indexToFollowColorBg=2;
    }
    var indexToLastColor=1;
    if(newIndex+2==slideSize){
     indexToLastColor=2;
    }
    jQuery('.contestPodSuspensionPointsLeft',parent).css('background-color',getColorBackground(((slideBelow3)?newIndex:(newIndex+indexToLastColor)),parent));
     jQuery('.contestPodSuspensionPointsRight',parent).css('background-color',getColorBackground(newIndex-indexToFollowColorBg,parent));
     jQuery('.contestPodLevelWrapper',parent).css('background-color',getColorBackground(((slideBelow3)?newIndex:newIndex+indexToLastColor),parent)); 
     jQuery('.contestPodNextTier',parent).css( {"visibility":''});
       jQuery('.contestPodPrevTier',parent).css( {"visibility":''});
    jQuery(jQuery('.cba',parent)[0]).removeClass('cactive'); 
    jQuery(jQuery('.cba',parent)[1]).addClass('cactive'); 
    jQuery(jQuery('.cba',parent)[2]).removeClass('cactive');  
   }  
   if(newIndex==slideSize){
    var indexToFollowColorBg=1;
    if(slideSize>3){
     indexToFollowColorBg=3;
    }else if(slideSize==3){
     indexToFollowColorBg=2;
    }
    jQuery('.contestPodSuspensionPointsLeft',parent).css('background-color',getColorBackground(newIndex,parent));
     jQuery('.contestPodSuspensionPointsRight',parent).css('background-color',getColorBackground(newIndex-((slideBelow3)?1:indexToFollowColorBg),parent));
     jQuery('.contestPodLevelWrapper',parent).css('background-color',getColorBackground(newIndex,parent));
    
    jQuery('.contestPodNextTier',parent).css( {"visibility":'hidden'});
       jQuery('.contestPodPrevTier',parent).css( {"visibility":''});
    jQuery(jQuery('.cba',parent)[0]).removeClass('cactive'); 
    jQuery(jQuery('.cba',parent)[1]).removeClass('cactive'); 
    jQuery(jQuery('.cba',parent)[2]).addClass('cactive'); 
   }
   //animate the slide position 
   //if is the first slide call when load the page and the newIndex is 1 we doesm't want to slide the tier container
   if(imperativeScroll && newIndex==1)return;
   //block this function until current animation finish 
    jQuery('.contestPodScrollSynchronize',jQuery(parent)).val('false');
      jQuery('.TiersScrolleable .items',jQuery(parent)).animate( {"left":newMargin},1000,"linear",
       function (){
        jQuery('.contestPodScrollSynchronize',jQuery(parent)).val('true');
       });
   }else{
    //if are only one tier we doesnt need to scroll, so doesn show suspension points, the background must be dark blue, arrow are not displayed and only one dot is display
     jQuery('.contestPodSuspensionPointsLeft',parent).html('');
    jQuery('.contestPodSuspensionPointsRight',parent).html('');
    jQuery('.contestPodNextTier',parent).css( {"visibility":'hidden'});
       jQuery('.contestPodPrevTier',parent).css( {"visibility":'hidden'});
    jQuery(jQuery('.cba',parent)[0]).removeClass('cactive').css( {"visibility":'hidden'}); 
    jQuery(jQuery('.cba',parent)[1]).addClass('cactive').css( {"visibility":'hidden'});  
    jQuery(jQuery('.cba',parent)[2]).removeClass('cactive').css( {"visibility":'hidden'});  
   }
 }
 function hasClass(element,stringClass){
  var strClasses=jQuery(element).attr('class');
  var arrClass=strClasses.split(" ");
  //console.info("=========/////==========\n")
  //console.info(stringClass);
  for(var i = 0 ; i < arrClass.length;i++){
   var str=arrClass[i].trim();
   //console.info('str > '+str);
   if(str==stringClass){
    return true;
   }
  }
  //console.info("==========/////=========\n")
  return false;
 }
 function getColorBackground(currentLevel,parent){
   var grey = hasClass(jQuery('.podRatelevel'+currentLevel,parent),'contestPodRateLevel');
   var blue = hasClass(jQuery('.podRatelevel'+currentLevel,parent),'contestPodRateLevelCurrent');
   var green =hasClass(jQuery('.podRatelevel'+currentLevel,parent),'contestPodRateLevelCurrentWon');
   //console.info('currentLevel '+currentLevel )
   //console.info(grey)
   //console.info(blue)
   //console.info(green)
   //console.info("===================\n")
   var bgColor = (green)?'#30af4c':((blue)?'#006EBE':'#cccccc');
   return bgColor;
 }
 
 //manage the colors of the levels backgrounds and the color of the suspension points 
 function manageBlueBackground(newIindex,fact,parent,slideSize){
  if(slideSize>3){
   if(newIindex<3){
    manageSuspensionDotsColor(parent);
    jQuery('.contestPodSuspensionPointsLeft',parent).html('...');
    jQuery('.contestPodSuspensionPointsRight',parent).html('');
   }else if(newIindex>=(slideSize-1)){
    manageSuspensionDotsColor(parent);
    jQuery('.contestPodSuspensionPointsRight',parent).html('...');
    jQuery('.contestPodSuspensionPointsLeft',parent).html('');
   }else{
    manageSuspensionDotsColor(parent);
    jQuery('.contestPodSuspensionPointsLeft',parent).html('...');
    jQuery('.contestPodSuspensionPointsRight',parent).html('...');
   }
  }else{
    jQuery('.contestPodSuspensionPointsLeft',parent).html('');
    jQuery('.contestPodSuspensionPointsRight',parent).html('');
  }
 }
 function manageSuspensionDotsColor(parent){
  if(jQuery('.contestPodSuspensionPointsRight',parent).css('background-color')!='#cccccc'){
