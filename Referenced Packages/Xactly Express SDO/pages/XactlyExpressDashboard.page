 //manage the position of the blue navigation bar
 function slideBluelevels(parent,newIindex,currentIndex,imperativeScroll){
  var slideSize=jQuery(".TiersScrolleable .slide",jQuery(parent)).size();
  var fact=(currentIndex<newIindex)?-1:1;
  manageBlueBackground(newIindex,fact,parent,slideSize);
  if(slideSize>3){  
    //if we are in the last or first index we doesnt must move
    if(slideSize==newIindex && !imperativeScroll)return; 
    if(1==newIindex)return;
    
    var diffIndex=(newIindex-currentIndex<0)?(currentIndex-newIindex):newIindex-currentIndex;
    //if you are in the first level and click in the second we doesnt need to move the blue bar
    if(fact<0 && newIindex==2)return;
    //if we are in the las element (fact>0) and click in the previus, we doesnt need to move
    if(fact>0 && newIindex==(slideSize-1))return;
    
    var currentPos=parseInt(jQuery('.contestPodLevelWrapper',parent).css('left').replace('px'));
    var numberOfScroll=0;
    var diffBase=(fact>0)?50:-50;
    
    if(diffIndex==1 || diffIndex==-1 ){
      numberOfScroll=diffBase;
    }else{
      numberOfScroll=(diffIndex-1)*diffBase;
    }
    var newPos=currentPos+numberOfScroll;
    if(imperativeScroll && slideSize==newIindex){
     newPos-=diffBase;
    }
    jQuery('.contestPodLevelWrapper',parent).animate( {"left":newPos},1000,"linear");
   }    
  }
